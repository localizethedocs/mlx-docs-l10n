#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/recurrent.py:docstring
#: of mlx.nn.layers.recurrent.LSTM:9
msgid "Concretely, for each element of the sequence, this layer computes:"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/recurrent.py:docstring
#: of mlx.nn.layers.recurrent.LSTM:11
msgid ""
"\\begin{aligned}\n"
"i_t &= \\sigma (W_{xi}x_t + W_{hi}h_t + b_{i}) \\\\\n"
"f_t &= \\sigma (W_{xf}x_t + W_{hf}h_t + b_{f}) \\\\\n"
"g_t &= \\text{tanh} (W_{xg}x_t + W_{hg}h_t + b_{g}) \\\\\n"
"o_t &= \\sigma (W_{xo}x_t + W_{ho}h_t + b_{o}) \\\\\n"
"c_{t + 1} &= f_t \\odot c_t + i_t \\odot g_t \\\\\n"
"h_{t + 1} &= o_t \\text{tanh}(c_{t + 1})\n"
"\\end{aligned}\n"
"\n"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/recurrent.py:docstring
