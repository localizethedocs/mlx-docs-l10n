msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/activations.py:docstring
#: of mlx.nn.layers.activations.selu:1
msgid "Applies the Scaled Exponential Linear Unit."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/activations.py:docstring
#: of mlx.nn.layers.activations.selu:3
msgid ""
"\\text{selu}(x) = \\begin{cases}\n"
"\\lambda x & \\text{if } x > 0 \\\\\n"
"\\lambda \\alpha (\\exp(x) - 1) & \\text{if } x \\leq 0\n"
"\\end{cases}\n"
"\n"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/activations.py:docstring
#: of mlx.nn.layers.activations.selu:9
msgid "where :math:`\\lambda = 1.0507` and :math:`\\alpha = 1.67326`."
msgstr ""

