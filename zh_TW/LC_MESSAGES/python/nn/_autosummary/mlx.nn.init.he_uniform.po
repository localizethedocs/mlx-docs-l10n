msgid "mlx.nn.init.he\\_uniform"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:1
msgid "A He uniform (Kaiming uniform) initializer."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:3
msgid ""
"This initializer samples from a uniform distribution with a range computed "
"from the number of input (``fan_in``) or output (``fan_out``) units "
"according to:"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:7
msgid "\\sigma = \\gamma \\sqrt{\\frac{3.0}{\\text{fan}}}"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:11
msgid ""
"where :math:`\\text{fan}` is either the number of input units when the "
"``mode`` is ``\"fan_in\"`` or output units when the ``mode`` is "
"``\"fan_out\"``."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:15
msgid ""
"For more details see the original reference: `Delving Deep into Rectifiers: "
"Surpassing Human-Level Performance on ImageNet Classification <https://arxiv."
"org/abs/1502.01852>`_"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:0
msgid "Parameters"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:20
msgid "The data type of the array. Default: ``float32``."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:0
msgid "Returns"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.he_uniform:23
msgid ""
"An initializer that returns an array with the same shape as the input, "
"filled with samples from  the He uniform distribution."
msgstr ""

