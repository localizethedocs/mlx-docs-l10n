#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/recurrent.py:docstring
#: of mlx.nn.layers.recurrent.GRU:9
msgid "Concretely, for each element of the sequence, this layer computes:"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/recurrent.py:docstring
#: of mlx.nn.layers.recurrent.GRU:11
msgid ""
"\\begin{aligned}\n"
"r_t &= \\sigma (W_{xr}x_t + W_{hr}h_t + b_{r}) \\\\\n"
"z_t &= \\sigma (W_{xz}x_t + W_{hz}h_t + b_{z}) \\\\\n"
"n_t &= \\text{tanh}(W_{xn}x_t + b_{n} + r_t \\odot (W_{hn}h_t + b_{hn})) \\"
"\\\n"
"h_{t + 1} &= (1 - z_t) \\odot n_t + z_t \\odot h_t\n"
"\\end{aligned}"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/layers/recurrent.py:docstring
