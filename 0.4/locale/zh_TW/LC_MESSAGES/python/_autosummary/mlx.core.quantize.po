msgid "Matrix to be quantized"
msgstr ""

#: ../../../docstring of
#: mlx.core.pybind11_detail_function_record_v1_system_libstdcpp_gxx_abi_1xxx_use_cxx11_abi_1.quantize:37
msgid ""
"The size of the group in ``w`` that shares a scale and bias. (default: "
"``64``)"
msgstr ""

#: ../../../docstring of
#: mlx.core.pybind11_detail_function_record_v1_system_libstdcpp_gxx_abi_1xxx_use_cxx11_abi_1.quantize:40
msgid ""
"The number of bits occupied by each element of ``w`` in the returned "
"quantized matrix. (default: ``4``)"
msgstr ""

#: ../../../src/python/_autosummary/mlx.core.quantize.rst:0
msgid "Returns"
msgstr ""

#: ../../../docstring of
#: mlx.core.pybind11_detail_function_record_v1_system_libstdcpp_gxx_abi_1xxx_use_cxx11_abi_1.quantize:44
