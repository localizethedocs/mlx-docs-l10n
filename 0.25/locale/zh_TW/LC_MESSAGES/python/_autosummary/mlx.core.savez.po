#: ../../../src/python/_autosummary/mlx.core.savez.rst:2
msgid "mlx.core.savez"
msgstr ""

#: ../../../docstring of mlx.core.savez:1
msgid "Save several arrays to a binary file in uncompressed ``.npz`` format."
msgstr ""

#: ../../../docstring of mlx.core.savez:4
msgid ""
"import mlx.core as mx\n"
"\n"
"x = mx.ones((10, 10))\n"
"mx.savez(\"my_path.npz\", x=x)\n"
"\n"
"import mlx.nn as nn\n"
"from mlx.utils import tree_flatten\n"
"\n"
"model = nn.TransformerEncoder(6, 128, 4)\n"
"flat_params = tree_flatten(model.parameters())\n"
"mx.savez(\"model.npz\", **dict(flat_params))"
msgstr ""

