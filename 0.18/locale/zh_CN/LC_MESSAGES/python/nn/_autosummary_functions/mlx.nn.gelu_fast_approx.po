msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/__init__.py:docstring
#: of mlx.nn.gelu_fast_approx:1
msgid "A fast approximation to Gaussian Error Linear Unit."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/__init__.py:docstring
#: of mlx.nn.gelu_fast_approx:3
msgid "See :func:`gelu` for the exact computation."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/__init__.py:docstring
#: of mlx.nn.gelu_fast_approx:5
msgid ""
"This function approximates ``gelu`` with a maximum absolute error :math:`< "
"0.015` in the range :math:`[-6, 6]` using the following"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/__init__.py:docstring
#: of mlx.nn.gelu_fast_approx:8
msgid "x = x \\sigma\\left(1.702 x\\right)"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/__init__.py:docstring
#: of mlx.nn.gelu_fast_approx:12
msgid "where :math:`\\sigma(\\cdot)` is the logistic sigmoid."
msgstr ""

