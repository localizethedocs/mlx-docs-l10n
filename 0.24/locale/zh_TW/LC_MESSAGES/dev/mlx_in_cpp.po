msgid "Package Variables"
msgstr "軟體包變數"

#: ../../../src/dev/mlx_in_cpp.rst:108
msgid "Variable"
msgstr "變數"

#: ../../../src/dev/mlx_in_cpp.rst:109
msgid "Description"
msgstr "說明"

#: ../../../src/dev/mlx_in_cpp.rst:110
msgid "MLX_FOUND"
msgstr ""

#: ../../../src/dev/mlx_in_cpp.rst:111
msgid "``True`` if MLX is found"
msgstr "若找到 MLX 則為 ``True``"

#: ../../../src/dev/mlx_in_cpp.rst:112
msgid "MLX_INCLUDE_DIRS"
msgstr ""

#: ../../../src/dev/mlx_in_cpp.rst:113
msgid "Include directory"
msgstr "包含目錄"

#: ../../../src/dev/mlx_in_cpp.rst:114
msgid "MLX_LIBRARIES"
msgstr ""

#: ../../../src/dev/mlx_in_cpp.rst:115
msgid "Libraries to link against"
msgstr "連結用程式庫"

#: ../../../src/dev/mlx_in_cpp.rst:116
msgid "MLX_CXX_FLAGS"
msgstr ""

#: ../../../src/dev/mlx_in_cpp.rst:117
msgid "Additional compiler flags"
msgstr "額外編譯器旗標"

#: ../../../src/dev/mlx_in_cpp.rst:118
msgid "MLX_BUILD_ACCELERATE"
msgstr ""

#: ../../../src/dev/mlx_in_cpp.rst:119
