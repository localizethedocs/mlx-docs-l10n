msgid "A Glorot uniform initializer."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.glorot_uniform:3
msgid ""
"This initializer samples from a uniform distribution with a range computed "
"from the number of input (``fan_in``) and output (``fan_out``) units "
"according to:"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.glorot_uniform:7
msgid ""
"\\sigma = \\gamma \\sqrt{\\frac{6.0}{\\text{fan\\_in} + \\text{fan\\_out}}}\n"
"\n"
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/init.py:docstring
#: of mlx.nn.init.glorot_uniform:10
msgid ""
"For more details see the original reference: `Understanding the difficulty "
"of training deep feedforward neural networks <https://proceedings.mlr.press/"
"v9/glorot10a.html>`_"
msgstr ""

