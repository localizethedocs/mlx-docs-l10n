#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/losses.py:docstring
#: of mlx.nn.losses.huber_loss:1
msgid "Computes the Huber loss between inputs and targets."
msgstr "計算輸入與目標之間的 Huber 損失。"

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/losses.py:docstring
#: of mlx.nn.losses.huber_loss:3
msgid ""
"l_{\\delta}(a) =\n"
"\\left\\{ \\begin{array}{ll}\n"
"    \\frac{1}{2} a^2 & \\text{for } |a| \\leq \\delta, \\\\\n"
"    \\delta \\left( |a| - \\frac{1}{2} \\delta \\right) & "
"\\text{otherwise.}\n"
"\\end{array} \\right."
msgstr ""

