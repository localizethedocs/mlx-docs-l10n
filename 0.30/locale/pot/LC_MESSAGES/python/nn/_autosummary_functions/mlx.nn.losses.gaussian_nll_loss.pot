msgid "The predicted expectation of the Gaussian distribution."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/losses.py:docstring
#: of mlx.nn.losses.gaussian_nll_loss:15
msgid "The target values (samples from the Gaussian distribution)."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/losses.py:docstring
#: of mlx.nn.losses.gaussian_nll_loss:17
msgid "The predicted variance of the Gaussian distribution."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/losses.py:docstring
#: of mlx.nn.losses.gaussian_nll_loss:19
msgid ""
"Whether to include the constant term in the loss calculation. Default: "
"``False``."
msgstr ""

#: ../../../../../../.conda/lib/python3.12/site-packages/mlx/nn/losses.py:docstring
#: of mlx.nn.losses.gaussian_nll_loss:22
